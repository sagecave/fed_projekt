---
import Image from "astro/components/Image.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import EmployeeSection from "../components/EmployeeSection.astro";
import FrontpageHero from "../components/FrontpageHero.astro";
import HeroCard from "../components/HeroCard.astro";
import ExpectSection from "../components/ExpectSection.astro";
import SectionFinancial from "../components/SectionFinancial.astro";
import CircleSection from "../components/CircleSection.astro";
import CorevaluesSection from "../components/CorevaluesSection.astro";
import ContactUsSection from "../components/ContactUsSection.astro";
import Newsletter from "../components/Newsletter.astro";
---

<BaseLayout title="Home">
  <FrontpageHero class="flow" />
  <ExpectSection
    class="flow"
    headerContent="What To Expect"
    pContent="Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat."
    liContentFirst="Bring to the table win-win survival strategies to ensure proactive domination. At the end of the day."
    liContentSecond="Capitalize on low hanging fruit to identify a ballpark value added activity to beta test override the digital."
  />
  <SectionFinancial class="flow" />
  <CircleSection class="flow" />
  <CorevaluesSection class="flow" />
  <ContactUsSection class="flow" />

  <Newsletter class="flow" />
</BaseLayout>
