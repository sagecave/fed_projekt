---
import Image from "astro/components/Image.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import EmployeeSection from "../components/EmployeeSection.astro";

const data_url = "https://raw.githubusercontent.com/sagecave/frontend-design/main/resources/json/employees.json";
const employeeData = await fetch(data_url).then((res) => res.json());
---

<BaseLayout title="Home">
  <EmployeeSection data={employeeData} theme="dark" />
</BaseLayout>
